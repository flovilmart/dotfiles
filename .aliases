code () {
	if [[ $# = 0 ]]
	then
		open_command -a "Visual Studio Code"
	else
		local argPath="$1"
		[[ $1 = /* ]] && argPath="$1"  || argPath="$PWD/${1#./}"
    open_command -a "Visual Studio Code" "$argPath"
	fi
}

alias mdev="mux dev"
alias tsw="tmux switch -t"
alias flush_dns="sudo killall -HUP mDNSResponder; sleep 2;"
alias set_pane_title="printf '\033]2;%s\033\\'"


alias docker_clean_volumes="docker volume rm $(docker volume ls -qf dangling=true)"
alias docker_clean_images="docker rmi $(docker images --filter "dangling=true" -q --no-trunc)"
alias docker_clean_all="docker_clean_volumes && docker_clean_images"
